{
   "type":"object",
   "propertyNames":{
      "pattern":"^[A-Za-z][A-Za-z_]*$"
   },
   "patternProperties":{
      "[a-zA-Z_]*":{
         "$ref":"#/definitions/toggle"
      }
   },
   "definitions":{
      "toggleEnum":{
         "enum":[
            "Disabled",
            "Enabled"
         ]
      },
      "toggle":{
         "anyOf":[
            {
               "$ref":"#/definitions/toggleEnum"
            },
            {
               "type":"object",
               "patternProperties":{
                  "[a-zA-Z]*":{
                     "$ref":"#/definitions/toggleEnum"
                  }
               }
            }
         ]
      }
   }
}
